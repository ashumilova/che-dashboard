
ide-iframe#ide-iframe-window.ng-hide
  top -100%
  opacity 1
  transition-delay 0s
  transition-property all
  transition-duration 0.7s
  transition-timing-function cubic-bezier(1, 0.835, 0, 0.945)

/*ide-iframe#ide-iframe-window.ng-hide-remove
  top -100%
*/


#ide-iframe-window
  position absolute
  width 100%
  height 100%
  top 0
  left 0
  z-index 60
  opacity 1
  overflow hidden
